  <builders>
    <hudson.tasks.Shell>
      <command>lastRev=`/usr/bin/svn list https://vc.fen.com/svn/popcomics/tags/release-candidates/1-0-x | /usr/bin/sort -V | /usr/bin/tail -n 1 | /usr/bin/sed &apos;s~/$~~&apos;`
branchPart=`echo $lastRev | sed &apos;s~[0-9]\+$~~&apos;`
subMinor=`echo $lastRev | sed &apos;s~.*\.~~&apos;`
nextRev=$branchPart$(($subMinor+1))
echo NEXT_REV=$nextRev &gt; temp.properties
echo SVN_URL=$SVN_URL &gt;&gt; temp.properties
echo SVN_REVISION=$SVN_REVISION &gt;&gt; temp.properties
echo BUILD_USER_ID=$BUILD_USER_ID &gt;&gt; temp.properties
echo BUILD_USER=$BUILD_USER &gt;&gt; temp.properties
echo MOBILE_RELEASE=$(echo ${nextRev:12}) &gt;&gt; temp.properties
echo LABEL=${majMin}0 &gt;&gt; temp.properties
echo BUILD_TYPE=${BUILD_TYPE} &gt;&gt; temp.properties</command>
    </hudson.tasks.Shell>
    <EnvInjectBuilder plugin="envinject@1.91">
      <info>
        <propertiesFilePath>temp.properties</propertiesFilePath>
      </info>
    </EnvInjectBuilder>
    <hudson.tasks.Ant plugin="ant@1.2">
      <targets>fetch package</targets>
      <antName>Ant</antName>
      <properties>release.version=${NEXT_REV}
release.environment=${release.environment}
build.type=${build.type}
AIR_SDK_HOME=${AIR_SDK_HOME}</properties>
    </hudson.tasks.Ant>
  </builders>
  <publishers>
    <hudson.tasks.ArtifactArchiver>
      <artifacts>build/*/bin/AndroidGame.apk,build/*/bin/IosGame.ipa</artifacts>
      <allowEmptyArchive>false</allowEmptyArchive>
      <onlyIfSuccessful>false</onlyIfSuccessful>
      <fingerprint>false</fingerprint>
      <defaultExcludes>true</defaultExcludes>
    </hudson.tasks.ArtifactArchiver>
    <hudson.plugins.svn__tag.SvnTagPublisher plugin="svn-tag@1.18-SNAPSHOT">
      <tagBaseURL>https://vc.fen.com/svn/popcomics/tags/release-candidates/1-0-x/${env[&apos;NEXT_REV&apos;]}</tagBaseURL>
      <tagComment>Release Candidate ${env[&apos;NEXT_REV&apos;]} by Jenkins svn-tag plugin. Build:${env[&apos;BUILD_TAG&apos;]}.</tagComment>
      <tagDeleteComment></tagDeleteComment>
    </hudson.plugins.svn__tag.SvnTagPublisher>
  </publishers>
