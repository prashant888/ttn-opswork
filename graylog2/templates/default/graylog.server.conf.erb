<%= config_option 'alert_check_interval', node.graylog2[:server][:alert_check_interval] -%>

# MongoDB Configuration
<% if node.graylog2[:major_version].to_f <= 1.0 -%>
<%= config_option 'mongodb_useauth', node.graylog2[:mongodb][:useauth] -%>
